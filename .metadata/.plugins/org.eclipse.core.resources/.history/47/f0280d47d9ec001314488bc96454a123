import java.util.Date;

public class Time {
    public static Date d = new Date();
    int hours;
    int mins;
    int secs;
    int day;
    int month;
    int year;
    
    public static void main(String[] args) {
        System.out.println(d.toString());
    }
    public Time(int hours, int mins, int secs, int day, int month, int year) {
        this.hours = hours;
        this.mins = mins;
    }

}
